{
    "name" : "mild/mild",
    "keywords": ["framework", "mild", "php"],
    "type": "project",
    "license": "MIT",
    "description": "The Mild PHP Framework",
    "authors": [
        {
            "name": "Mochammad Riyadh Ilham Akbar Pasya",
            "email": "ilhampasya220317@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\" : "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "mild/framework": "^1.0",
        "nesbot/carbon": "^2.11"
    },
    "require-dev": {
        "psy/psysh": "^0.9.9",
        "symfony/console": "^4.2",
        "fzaninotto/faker": "^1.8",
        "nunomaduro/collision": "^2.1",
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
